def getMinJumps(arr, n):
    jumps = 0
    maxfromlist = 0
    timetojump = arr[0]
    if timetojump == 0:
        return -1
    for i in range(1, n ):
        greatest = arr[i] + i
        if timetojump >= n-1:
            jumps+=1
            break

        if maxfromlist == 0:
            maxfromlist = greatest

        if greatest>maxfromlist:
            maxfromlist = greatest

        if i == timetojump:
            jumps +=1
            timetojump = maxfromlist
            if timetojump<= i:
                return -1
            maxfromlist = arr[i+1] + i+1
            if maxfromlist == timetojump:
                maxfromlist = 0
    return jumps
print(getMinJumps([1, 3, 5, 8, 9, 2, 6, 7, 6, 8, 9 ], 11))
